root = true

[*]
indent_style = tab
indent_size = 4
tab_width = 4
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{c,h,cpp,hpp}]
indent_style = tab
indent_size = 4
tab_width = 4

[components/**]
indent_style = unset
indent_size = unset
tab_width = unset
trim_trailing_whitespace = unset
insert_final_newline = unset
